{
    "openapi": "3.0.0",
    "info": {
        "version": "0.0.7",
        "title": "SPINE API",
        "description": "SPINE API"
    },
    "servers": [
        {
            "url": "{{SPINEUrl}}/api"
        }
    ],
    "paths": {
        "/workflow-execution-matrix": {
            "$ref": "./workflowExecutionMatrix/workflowExecutionMatrix.json"
        },
        "/workflow-execution-matrix/{workflowExecutionMatrixId}": {
