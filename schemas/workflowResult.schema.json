{
  "$id":"https://raw.githubusercontent.com/SPINEProject/SPINE-json-schema/master/schemas/workflowResult.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Workflow Result",
  "properties": {
    "uuid":{
      "type":"string",
      "description":"UUID of the result doc"
    },
    "results": {
      "description":"The results",
      "type":"object",
      "patternProperties" : {
        ".*" : {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "value":{
              "description":"Value of the result",
            }
          }
        }
      }
    },
    "reference" :{
      "type":"object",
      "additionalProperties": false,
      "properties": {
        "workflowExecutorId":{
          "type":"string",
          "description":"Reference to the workflow executor",
        },
        "workflowId":{
          "type":"string",
          "description":"Reference to the workflow",
        }
      },
      "required":["workflowId", "workflowExecutorId"]
    }
  },
  "additionalProperties": false,
  "required":["uuid", "reference", "results"]
}
